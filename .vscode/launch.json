    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug bin 's4' gen-99999-1-Motley_Crue.log",
            "cargo": {
                "args": [
                    "build",
                    "--bin=s4",
                    "--package=super_speedy_syslog_searcher"
                ],
                "filter": {
                    "name": "s4",
                    "kind": "bin"
                }
            },
            "args": [
                "./logs/other/tests/gen-99999-1-Motley_Crue.log",
            ],
            "cwd": "${workspaceFolder}",
            "sourceLanguages": [
                "rust"
            ],
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in bin 's4'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=s4",
                    "--package=super_speedy_syslog_searcher"
                ],
                "filter": {
                    "name": "s4",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}",
            "sourceLanguages": [
                "rust"
            ],
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Run release 's4' .etl file",
            "cargo": {
                "args": [
                    "run",
                    "--profile=flamegraph",
                    "--",
                    "./logs/programs/etl/waasmedic.20251005_113019_195.etl"
                ],
                "filter": {
                    "name": "s4",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}",
            "sourceLanguages": [
                "rust"
            ],
        },
        {
            "type": "debugpy",
            "request": "launch",
            "name": "Run etl_reader_etl_parser.py",
            "program": "${workspaceFolder}/src/python/s4_etl_readers/s4_etl_readers/etl_reader_etl_parser.py",
            "args": [
                "${workspaceFolder}/logs/programs/etl/waasmedic.20251005_113019_195.etl",
            ],
            "cwd": "${workspaceFolder}",
            "sourceLanguages": [
                "python"
            ],
        }
    ]
}